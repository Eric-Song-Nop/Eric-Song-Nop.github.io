{
	"name": "Eric-Song-Nop.github.io",
	"version": "1.0.0",
	"description": "A modern TypeScript project using pnpm and Biome",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/index.js",
		"build:docs": "scip-typescript index && cire -c .cire.json5",
		"lint": "biome check",
		"lint:fix": "biome check --fix",
		"format": "biome format",
		"format:write": "biome format --fix",
		"clean": "rm -rf dist",
		"type-check": "tsc --noEmit"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.19.0",
	"devDependencies": {
		"@biomejs/biome": "2.2.7",
		"@cire-doc/cire-cli": "^0.1.0",
		"@types/node": "^24.10.1",
		"typescript": "^5.9.3"
	}
}
